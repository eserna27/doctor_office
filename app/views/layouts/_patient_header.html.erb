<section class="page-header">
  <div class="row page-header-principal">
    <div class="col">
      <h2>
        <%= title %>
      </h2>
    </div>
    <div class="col">
    </div>
  </div>
</section>
<% if alert %>
  <div class="alert alert-primary" role="alert">
    <%= alert %>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  </br>
<% end %>
